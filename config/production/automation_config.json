{
  "share_email": "REPLACED_EMAIL",
  "automation_enabled": true,
  "default_pull_type": "daily",
  "data_granularity": "15_minute",
  "google_drive_sync": true,
  "notifications": {
    "email_on_success": false,
    "email_on_failure": true,
    "email_address": "REPLACED_EMAIL"
  },
  "data_retention": {
    "keep_raw_data_months": 24,
    "keep_processed_data_months": 12,
    "auto_cleanup": false
  },
  "schedules": {
    "daily_pull": {
      "enabled": true,
      "time": "06:00",
      "sources": ["wu", "tsi"],
      "data_granularity": "15_minute"
    },
    "weekly_pull": {
      "enabled": false,
      "day_of_week": "monday",
      "time": "06:00",
      "sources": ["wu", "tsi"]
    },
    "monthly_summary": {
      "enabled": true,
      "day_of_month": 1,
      "time": "07:00",
      "generate_reports": true
    }
  }
}
